

  <% if p.favorited_by?(current_user) %>
    <%= link_to parking_favorites_path(p), method: :delete, remote: true do %>
    <i class="fas fa-heart"></i>
    <% end %>
  <% else %>
    <%= link_to parking_favorites_path(p), method: :post, remote: true do %>
    <i class="far fa-heart"></i>
    <% end %>
  <% end %>
